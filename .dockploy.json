{
  "name": "companyos-backend",
  "description": "ENEA Telecom Backend - NestJS with Prisma",
  "dockerfile": "Dockerfile",
  "port": 8000,
  "buildArgs": {
    "NODE_ENV": "production"
  },
  "healthCheck": {
    "path": "/health",
    "interval": 30,
    "timeout": 10,
    "retries": 3,
    "startPeriod": 40
  },
  "environment": {
    "NODE_ENV": "production"
  },
  "volumes": {
    "uploads": "/app/uploads"
  },
  "restart": "unless-stopped"
}
